{
  "name": "@dpi/adonis-backup-helper",
  "version": "1.0.3",
  "description": "AdonisJS Backup Helper",
  "main": "build/providers/BackupProvider.js",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts",
    "clean": "del-cli build",
    "copyfiles": "copyfiles \"templates**/*.txt\" build",
    "compile": "npm run lint && npm run clean && tsc && npm run copyfiles",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "repository": "https://github.com/dpibrasil/adonis-backup-helper.git",
  "author": "Toni Moreira <moreiraatoni@gmail.com>",
  "license": "MIT",
  "private": false,
  "peerDependencies": {
    "@adonisjs/core": "^5.1.0",
    "@adonisjs/lucid": "^18.0.1"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [],
    "minNodeVersion": "16.13.1",
    "probotApps": []
  },
  "files": [
    "build/src",
    "build/index.d.ts",
    "build/instructions.js",
    "build/index.js",
    "build/adonis-typings",
    "build/commands",
    "build/providers",
    "build/src",
    "build/templates"
  ],
  "dependencies": {
    "@adonisjs/lucid": "^18.0.1",
    "@adonisjs/sink": "^5.4.2",
    "adm-zip": "^0.5.9",
    "copyfiles": "^2.4.1"
  },
  "commands": [
    "@dpi/adonis-backup-helper/commands"
  ],
  "adonisjs": {
    "providers": [
      "@dpi/adonis-backup-helper"
    ],
    "types": "@dpi/adonis-backup-helper",
    "instructions": "./build/instructions.js",
    "templates": {
      "basePath": "./build/templates",
      "config": [
        "backup.txt"
      ]
    }
  },
  "yalcSig": "811df3b0530acf611bfcbb214dc73bcd"
}
